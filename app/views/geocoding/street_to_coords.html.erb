<div class="container">
  <div class="row">

  <%= render "layouts/sidebar" %>

    <div class="col-md-9">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title">Street to Coordinates results</h3>
        </div>
        <div class="panel-body">
            <dl>
              <dt>Street Address</dt>
              <dd><%= @street_address %></dd>

              <dt>Latitude</dt>
              <dd><%= @latitude %></dd>

              <dt>Longitude</dt>
              <dd><%= @longitude %></dd>
            </dl>
        </div>
      </div>
    </div>
  </div>
</div>
